{
  "name": "template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "npx cross-env NODE_OPTIONS=--max_old_space_size=1536 && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@event-calendar/core": "^2.5.1",
    "@event-calendar/day-grid": "^2.5.1",
    "@event-calendar/interaction": "^2.5.1",
    "@event-calendar/time-grid": "^2.5.1",
    "@fontsource-variable/open-sans": "^5.0.21",
    "@fontsource/source-sans-pro": "^5.0.8",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-node": "^2.1.2",
    "@sveltejs/kit": "^2.4.3",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/cookie": "^0.5.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte": "^2.35.1",
    "flowbite": "^2.2.1",
    "flowbite-svelte": "^0.44.21",
    "flowbite-svelte-icons": "^0.4.5",
    "marked": "^11.1.0",
    "postcss": "^8.4.32",
    "postcss-load-config": "^4.0.2",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.0.0"
  },
  "type": "module",
  "dependencies": {
    "cookie": "^0.5.0"
  }
}
